
  <li class="review">
    <br>
<% if (YelpClone.loggedIn() && YelpClone.currentUser.reviews().get(review.id)) { %>
      <a href=<%= "#reviews/" + review.id + "/" + review.get("businessid") + "/edit" %>>Edit</a>
    <% } %>
    <a href=<%= "#business/" + review.get("businessid") %>>
      <%= review.escape("businessname") %>
    </a>
    <br>
    Rating:<%= review.escape("rating") %>
    <br>
    <%= review.escape("content") %>
    <br>
  </li>
